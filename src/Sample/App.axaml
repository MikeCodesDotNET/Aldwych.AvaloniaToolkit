<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:SampleApp"
             x:Class="SampleApp.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
        <StyleInclude Source="avares://Aldwych.AvaloniaToolkit/Themes/AldwychTheme.xaml" />
        <StyleInclude Source="avares://Aldwych.Mdi/Themes/MdiWindow.xaml" />

    </Application.Styles>
</Application>
